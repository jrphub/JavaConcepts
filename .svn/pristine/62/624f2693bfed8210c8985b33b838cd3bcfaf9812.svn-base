package com.java.comparator;

import java.util.Comparator;

public class SizeComparator implements Comparator<HDTV>{

	@Override
	public int compare(HDTV o1, HDTV o2) {
		
		//Ascending order
		return o1.getSize() - o2.getSize();
		
		//Descending Order
		//return o2.getSize() - o1.getSize();
		
	}

}
